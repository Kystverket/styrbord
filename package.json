{
  "name": "@kystverket/styrbord",
  "version": "0.300.3",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/main.d.ts",
      "style": "./dist/style.css",
      "import": "./dist/main.js"
    },
    "./*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "main": "./dist/main.js",
  "typings": "./dist/src/main.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "dev": "vite",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "build:css": "sass src/css/index.scss dist/index.css --style=compressed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "pretty:check": "npx prettier . --check",
    "pretty:fix": "npx prettier . --write",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "lint:pkg": "npm exec -- publint"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@digdir/designsystemet": "^1.0.1",
    "@digdir/designsystemet-css": "^1.0.1",
    "@digdir/designsystemet-react": "^1.0.1",
    "@digdir/designsystemet-theme": "^1.0.1",
    "@navikt/aksel-icons": "^6.17.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@kystverket/styrbord-tokens": "^0.2.26",
    "@chromatic-com/storybook": "^1.9.0",
    "@eslint/js": "^9.22.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/manager-api": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/theming": "^8.6.4",
    "@storybook/types": "^8.6.4",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^20.17.24",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "clsx": "^2.1.1",
    "eslint": "^9.22.0",
    "eslint-plugin-css": "^0.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "^0.11.4",
    "globals": "^13.24.0",
    "jiti": "^2.4.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "publint": "^0.2.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "sass": "^1.85.1",
    "storybook": "^8.6.4",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^5.4.14"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "esbuild": "^0.24.2",
    "material-symbols": "^0.23.0",
    "react-datepicker": "^8.2.0"
  },
  "//": "Override is a workaround until issue is resolved for typescript > 5.8.0 - https://github.com/storybookjs/eslint-plugin-storybook/issues/193",
  "overrides": {
    "eslint-plugin-storybook": {
      "typescript": "$typescript"
    }
  }
}
